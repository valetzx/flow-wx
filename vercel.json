{
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://wx-api-deno.onmicrosoft.cn/api/$1",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET,POST"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/ideas",
      "destination": "/ideas.html"
    },
    {
      "source": "/add",
      "destination": "/add.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
