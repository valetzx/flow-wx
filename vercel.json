{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://wx-api-deno.onmicrosoft.cn/$1",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, POST"
      }
    },
    {
      "src": "/ideas",
      "dest": "/ideas.html"
    },
    {
      "src": "/add",
      "dest": "/add.html"
    }
  ]
}
