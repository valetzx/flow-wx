{
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "http://wx-api-deno.onmicrosoft.cn/$1"
    }
  ],
  "rewrites": [
    {
      "source": "/ideas",
      "destination": "/ideas.html"
    },
    {
      "source": "/add",
      "destination": "/add.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
