{
  "version": 2,
  "routes": [
    {
      "src": "/api/plugins",
      "dest": "/api/plugins"
    },
    {
      "src": "/api/(.*)",
      "dest": "https://pigeonpig.github.io/api/$1",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, POST",
        "Cache-Control": "public, max-age=36000"
      }
    },
    {
      "src": "/a/(.*)",
      "dest": "https://wx-api-deno.onmicrosoft.cn/a/$1",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, POST",
        "Cache-Control": "public, max-age=360000"
      }
    },
    {
      "src": "/ideas",
      "dest": "/ideas.html"
    },
    {
      "src": "/game21",
      "dest": "/game21.html"
    },
    {
      "src": "/add",
      "dest": "/add.html"
    }
  ]
}
